{% extends 'base.html.twig' %}

{% block title %}Admission index{% endblock %}

{% block body %}
    <h1>Admission index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Demande_insc</th>
                <th>Cin</th>
                <th>Bac</th>
                <th>Att_travail</th>
                <th>Att_titulaire</th>
                <th>Autorisation</th>
                <th>Cv</th>
                <th>CinFile</th>
                <th>BacFile</th>
                <th>Att_travailFile</th>
                <th>Att_titulaireFile</th>
                <th>AutorisationFile</th>
                <th>CvFile</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for admission in admissions %}
            <tr>
                <td>{{ admission.id }}</td>
                <td>{{ admission.demandeInsc ? 'Yes' : 'No' }}</td>
                <td>{{ admission.cin }}</td>
                <td>{{ admission.bac }}</td>
                <td>{{ admission.attTravail }}</td>
                <td>{{ admission.attTitulaire }}</td>
                <td>{{ admission.autorisation }}</td>
                <td>{{ admission.cv }}</td>
                <td>{{ admission.cinFile }}</td>
                <td>{{ admission.bacFile }}</td>
                <td>{{ admission.attTravailFile }}</td>
                <td>{{ admission.attTitulaireFile }}</td>
                <td>{{ admission.autorisationFile }}</td>
                <td>{{ admission.cvFile }}</td>
                <td>
                    <a href="{{ path('admission_show', {'id': admission.id}) }}">show</a>
                    <a href="{{ path('admission_edit', {'id': admission.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="15">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('admission_new') }}">Create new</a>
{% endblock %}
